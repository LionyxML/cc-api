  "author": "LionyxML <rahul.juliato@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.17.0",
    "yarn": ">=1.22.15",
    "npm": "PLEASE_USE_YARN"
  },
  "scripts": {
    "start": "tsc && NODE_ENV=\"production\" node ./dist/index.js",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/index.ts",
    "build": "tsc",
    "prettier": "prettier --check 'src/**/*.{js,ts}'",
